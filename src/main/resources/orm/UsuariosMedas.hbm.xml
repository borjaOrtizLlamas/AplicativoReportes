<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.connectis.model.UsuariosMedas" table="USUARIOS_MEDAS"	catalog="system">

		<id name="usuario" type="string">
			<column name="usuario" length="45" />
		</id>

		<property name="contrasena" type="string">
			<column name="contrasena" not-null="false" />
		</property>

		<property name="valido" type="int">
			<column name="valido" not-null="false" />
		</property>
		
		<property name="nombre" type="string">
			<column name="nombre" not-null="false" />
		</property>
		
		<property name="telefono" type="int">
			<column name="telefono" not-null="false" />
		</property>
				
		<property name="extension" type="int">
			<column name="extension" not-null="false" />
		</property>		
		
		<property name="movil" type="int">
			<column name="movil" not-null="false" />
		</property>
		<property name="email" type="string">
			<column name="email" not-null="false" />
		</property>		
		
	</class>
</hibernate-mapping>
